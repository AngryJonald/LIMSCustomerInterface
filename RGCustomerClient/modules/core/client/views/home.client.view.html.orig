<section ng-controller="HomeController" data-ng-init="getUserProjects()">
	<div ng-controller="GraphController">
		<div class="text-center">
			<div class="row" ng-hide="authentication.user">
				<div class="col-md-6 col-md-offset-3 col-sm-6 col-sm-offset-3 col-xs-12">
					<img alt="MEAN.JS" height="500" src="modules/core/client/img/brand/rapid_logo_new_background.png" />
					<p ng-hide="authentication.user">
						<a class="btn btn-primary btn-lg outline card" ui-sref="signin">Log In</a>
						<a class="btn btn-primary btn-lg outline card" ui-sref="signup">Sign Up</a>
					</p>
					<br>
					<div class="row">
						<p class="lead">
							High-throughput, flexible and affordable DNA analysis solutions. RAPiD Genomics offers DNA genotyping and data analysis solutions that suits your unique applications.
						</p>
					</div>
					<br>
				</div>
			</div>
			<div ng-show="authentication.user">
				<br>
				<br>
				<br>
				<h3 class="cardtitle">Projects</h3>
				<ul class="nav nav-pills nav-justified">
					<li ng-repeat="project in projects">
						<button class="btn btn-primary btn-lg outline card" ng-click="switchProject(project); getUserProjects();">{{project.projectCode}}</button>
					</li>
				</ul>
				<br>
				<br>
				<div class="infocard-large card text-center">
					<h3 class="cardtitle">Progress</h3>
					<br>
					<div id="graphDiv" style="max-width = 80%"> </div>
				</div>
				<div class="infocard-left card">
					<h3 class="cardtitle">Status</h3>
					<p class="info-pretty">
						<br> Created: {{dateCreate}}
						<br> Due: {{dateDue}}
						<br> Last Edit: {{dateLastEdit}}
						<br> Arrival: {{dateArrive}}
					</p>
				</div>
				<div class="infocard-right card">
					<h3 class="cardtitle">Other</h3>
					<p class="info-pretty">
						<br> Organism: {{currOrganism}}
						<br> Stage: {{currStage}}
						<br> Probes: {{currProbes}}
						<br> Total Probes: {{currTotProbes}}
						<br> Shearing Method: {{currShearing}}
						<br> Total Sequencing: {{currSeq}}
						<br> Machine: {{currMachine}}
					</p>
				</div>
				<div class="infocard-left-temp card style=" margin:auto; "">
					<h3 class="cardtitle">Plates</h3>
					<br>
					<div id="plate0" class="plate-image"></div>
					<div id="plate1" class="plate-image"></div>
					<div id="plate2" class="plate-image"></div>
					<div id="plate3" class="plate-image"></div>
					<div id="plate4" class="plate-image"></div>
					<div id="plate5" class="plate-image"></div>
					<div id="plate6" class="plate-image"></div>
					<div id="plate7" class="plate-image"></div>
					<div id="plate8" class="plate-image"></div>
				</div>

				<div class="infocard-right-temp card">
					<h3 class="cardtitle">Plate Stages</h3>
					<p class="info-pretty">
						<br> Plate 1 Stage: {{plate0Stg}}
						<br> Plate 2 Stage: {{plate1Stg}}
						<br> Plate 3 Stage: {{plate2Stg}}
						<br> Plate 4 Stage: {{plate3Stg}}
						<br> Plate 5 Stage: {{plate4Stg}}
						<br> Plate 6 Stage: {{plate5Stg}}
						<br> Plate 7 Stage: {{plate6Stg}}
						<br> Plate 8 Stage: {{plate7Stg}}
						<br> Plate 9 Stage: {{plate8Stg}}
					</p>
				</div>
			</div>
		</div>
	</div>
	<div data-ng-show="showPlates" data-ng-controller="ProjectPlatesController" class="bs-callout shadow">
		<h3 style="margin: 2px">Plates</h3>
		<table class="table table-hover">
			<tr>
				<td>Plate Code</td>
				<td>Number of Samples</td>
				<td>Stage</td>
				<td>Detail</td>
			</tr>
			<tr data-ng-repeat="plate in plates | orderBy: 'plateCode' " data-ng-class="{'success': isPlateSelected(plate), 'danger': (plate.isAssigned && plateTableMode.mode === 'AddToWorklist') }" data-ng-click="selectPlate(plate)" class="pointer-table-row">
				<td>{{plate.plateCode}}</td>
				<td>{{plate.samples.length}}</td>
				<td>{{converter.convertFromNumber(plate.stage)}}</td>
				<td>
					<plate-view ng-attr-quick-view-settings="quickViewSettings" ng-attr-plate-code="plate.plateCode" ng-attr-color-functions="colorFunctions"></plate-view>
				</td>
			</tr>
		</table>
	</div>
</section>
