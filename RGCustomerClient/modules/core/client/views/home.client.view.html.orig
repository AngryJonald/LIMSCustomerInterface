						<td>{{currSeq}}</td>
					</tr>
					<tr>
						<th>Machine</th>
						<td>{{currMachine}}</td>
					</tr>
				</table>
			</div>
			<div ng-show="projectFinancesAccess" class="infocard card">
				<h3 class="cardtitle">Financial Information</h3>
			</div>
		</div>
	</div>
